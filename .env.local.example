# Frontend Environment Variables for Local Development
# Copy this file to .env.local and update with your local values

# API Configuration
# Point to your local backend services
VITE_API_URL=http://localhost:4000
VITE_POS_API_URL=http://localhost:5000
VITE_ADMIN_API_URL=http://localhost:4000

# Authentication
VITE_JWT_STORAGE_KEY=accessToken

# Feature Flags
VITE_ENABLE_LOGGING=true
VITE_LOG_LEVEL=debug

# Application URLs
VITE_APP_NAME=Eat With Me
VITE_APP_VERSION=0.1.0

# Development Settings
VITE_DEV_MODE=true
